# API Keys
OPENAI_API_KEY=your_openai_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
GROQ_API_KEY=your_groq_api_key_here  # Optional: for faster inference

# PostgreSQL Configuration (Required)
# Use agnohq/pgvector:16 Docker image for PgVector support
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/jee_agent

# Model Configuration
PRIMARY_MODEL=mistral/mistral-large-latest
FALLBACK_MODEL=groq/llama-3.3-70b-versatile
FAST_MODEL=mistral/open-mistral-nemo
EMBEDDING_MODEL=mistral-embed

# JEE Exam Configuration
EXAM_DATE=2026-01-23
TOTAL_MARKS=300
QUESTIONS_PER_SUBJECT=25

# Session Configuration
MAX_SESSION_HOURS=3
BREAK_INTERVAL_MINS=90
DEFAULT_DAILY_HOURS=10

# Logging (Optional)
LOG_LEVEL=INFO
